<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane maxWidth="600.0" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/9.0.4" xmlns:fx="http://javafx.com/fxml/1" fx:controller="client.controller.anagraficacontroller">
   <children>
      <ImageView fx:id="backhome" fitHeight="50.0" fitWidth="40.0" onMouseClicked="#backtohome" pickOnBounds="true" preserveRatio="true" AnchorPane.leftAnchor="10.0" AnchorPane.topAnchor="10.0">
         <image>
            <Image url="@../image/back2.png" />
         </image>
      </ImageView>
      <ImageView fitHeight="50.0" fitWidth="150.0" pickOnBounds="true" preserveRatio="true" AnchorPane.rightAnchor="10.0" AnchorPane.topAnchor="10.0">
         <image>
            <Image url="@../image/anagrafica2.png" />
         </image>
      </ImageView>
      <VBox alignment="CENTER" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="50.0">
         <children>
            <BorderPane fx:id="mainpane" maxHeight="400.0" maxWidth="600.0" prefHeight="400.0" prefWidth="600.0">
               <left>
                  <VBox alignment="TOP_CENTER" prefHeight="349.0" prefWidth="120.0" spacing="10.0" BorderPane.alignment="CENTER">
                     <children>
                        <Button fx:id="aggiungibutton" alignment="CENTER" mnemonicParsing="false" onAction="#gotoaggiungipersona" prefWidth="110.0" style="-fx-background-color: #f18e32 #f18e32;" text="aggiungi" textFill="WHITE">
                           <font>
                              <Font size="18.0" />
                           </font>
                        </Button>
                        <Button fx:id="visualizzabutton" alignment="CENTER" mnemonicParsing="false" onAction="#gotovisualizzapersona" prefWidth="110.0" style="-fx-background-color: #f18e32 #f18e32;" text="visualizza" textFill="WHITE">
                           <font>
                              <Font size="18.0" />
                           </font>
                        </Button>
                        <Button fx:id="modificabutton" alignment="CENTER" mnemonicParsing="false" onAction="#gotomodificapersona" prefWidth="110.0" style="-fx-background-color: #f18e32 #f18e32;" text="modifica" textFill="WHITE">
                           <font>
                              <Font size="18.0" />
                           </font>
                        </Button>
                        <Button fx:id="cancellabutton" alignment="CENTER" mnemonicParsing="false" prefWidth="110.0" style="-fx-background-color: #f18e32 #f18e32;" text="cancella" textFill="WHITE">
                           <font>
                              <Font size="18.0" />
                           </font>
                        </Button>
                     </children>
                     <padding>
                        <Insets top="10.0" />
                     </padding>
                  </VBox>
               </left>
               <padding>
                  <Insets bottom="1.0" left="1.0" right="1.0" top="1.0" />
               </padding>
            </BorderPane>
         </children>
      </VBox>
   </children>
</AnchorPane>
