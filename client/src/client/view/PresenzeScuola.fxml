<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane fx:id="PresenzeScuolaPane" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/9.0.4" xmlns:fx="http://javafx.com/fxml/1" fx:controller="client.controller.PresenzeScuolaController">
   <children>
      <VBox alignment="CENTER" spacing="20.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
         <children>
            <AnchorPane fx:id="QRPane" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="100.0" prefWidth="120.0" />
            <AnchorPane prefHeight="200.0" prefWidth="100.0">
               <children>
                  <TableView fx:id="presenzeTable" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="70.0" AnchorPane.rightAnchor="70.0" AnchorPane.topAnchor="0.0">
                     <columns>
                        <TableColumn fx:id="nomeColumn" prefWidth="75.0" text="Nome" />
                        <TableColumn fx:id="cognomeColumn" prefWidth="75.0" text="Cognome" />
                        <TableColumn fx:id="cfColumn" prefWidth="75.0" text="Codice fiscale" />
                     </columns>
                     <columnResizePolicy>
                        <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
                     </columnResizePolicy>
                  </TableView>
               </children>
            </AnchorPane>
            <Button fx:id="fineButton" alignment="TOP_CENTER" minWidth="120.0" mnemonicParsing="false" onAction="#returnToControlloAccessi" opacity="0.9" prefWidth="120.0" style="-fx-background-color: #f18e32;" text="fine" textAlignment="CENTER" textFill="#fcfcfc">
               <font>
                  <Font size="18.0" />
               </font>
            </Button>
         </children>
         <padding>
            <Insets bottom="10.0" top="10.0" />
         </padding>
      </VBox>
   </children>
</AnchorPane>
